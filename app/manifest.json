{
    "name": "Mooltipass ",
    "description": "Mooltipass password manager client",
    "version": "0.6",
    "app": {
        "background": {
            "scripts": [
                "js/init.js"
            ]
        }
    },
    "permissions": [
        "hid",
        {
            "usbDevices": [
                { "vendorId": 5840, "productId": 2464 }
            ]
        },
	   { "fileSystem": ["write"] },
	   "syncFileSystem"
    ],
    "icons": { 
        "19": "images/icons/AppIcon_19.png",
        "32": "images/icons/AppIcon_32.png",
        "128": "images/icons/AppIcon_128.png"
    }
}
