{
  "name": "Mooltipass App",
  "description": "Mooltipass password manager client",
  "version": "2.0.66",
  "app": {
    "background": {
      "scripts": [
        "vendor/mooltipass/helpers.js",
        "js/init.js"
      ]
    }
  },
  "permissions": [
    "hid",
    {
      "usbDevices": [
        {
          "vendorId": 5840,
          "productId": 2464
        }
      ]
    },
    {
      "fileSystem": [
        "write"
      ]
    },
    "syncFileSystem",
    "storage"
  ],
  "icons": {
    "19": "images/icons/AppIcon_19.png",
    "32": "images/icons/AppIcon_32.png",
    "128": "images/icons/AppIcon_128.png"
  }
}