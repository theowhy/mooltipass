{
    "name": "Mooltipass Client",
    "description": "Mooltipass password manager client",
    "version": "0.1",
    "app": {
        "background": {
            "scripts": ["main.js"]
        }
    },
    "permissions": [
        "hid",
        {
            "usbDevices": [
                { "vendorId": 5840, "productId": 2464 }
            ]
        },
	{"fileSystem": ["write", "retainEntries"]} 
    ],
    "icons": { "16": "mooltipass-16.png", "128": "mooltipass-128.png" }
}
